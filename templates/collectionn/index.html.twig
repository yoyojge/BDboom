{% extends 'base.html.twig' %}

{% block title %}Collectionn index{% endblock %}

{% block body %}
    <h1>Collectionn index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CollectionName</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for collectionn in collectionns %}
            <tr>
                <td>{{ collectionn.id }}</td>
                <td>{{ collectionn.CollectionName }}</td>
                <td>
                    <a href="{{ path('app_collectionn_show', {'id': collectionn.id}) }}">show</a>
                    <a href="{{ path('app_collectionn_edit', {'id': collectionn.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_collectionn_new') }}">Create new</a>
{% endblock %}
